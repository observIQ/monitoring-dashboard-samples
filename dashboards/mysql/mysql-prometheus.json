{
  "category": "CUSTOM",
  "dashboardFilters": [
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "cluster",
      "templateVariable": "Cluster"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "location",
      "templateVariable": "Location"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "namespace",
      "templateVariable": "Namespace"
    }
  ],
  "displayName": "MySQL Prometheus Overview",
  "labels": {},
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "height": 3,
        "widget": {
          "scorecard": {
            "gaugeView": {
              "upperBound": 1440
            },
            "thresholds": [
              {
                "color": "YELLOW",
                "direction": "BELOW",
                "label": "",
                "value": 30
              }
            ],
            "timeSeriesQuery": {
              "prometheusQuery": "mysql_global_status_uptime{${Cluster},${Location},${Namespace}} / 60"
            }
          },
          "title": "Global Status Uptime (minutes)"
        },
        "width": 5,
        "xPos": 0,
        "yPos": 0
      },
      {
        "height": 3,
        "widget": {
          "title": "MySQL Up",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "mysql_up{${Cluster},${Location},${Namespace}}"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 7,
        "xPos": 5,
        "yPos": 0
      },
      {
        "height": 3,
        "widget": {
          "title": "Global Status Connection Error Rate (5m)",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate(mysql_global_status_connection_errors_total{${Cluster},${Location},${Namespace}}[5m])"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 7,
        "xPos": 5,
        "yPos": 3
      },
      {
        "height": 3,
        "widget": {
          "scorecard": {
            "gaugeView": {
              "upperBound": 100
            },
            "thresholds": [
              {
                "color": "RED",
                "direction": "ABOVE",
                "label": "",
                "value": 85
              },
              {
                "color": "YELLOW",
                "direction": "ABOVE",
                "label": "",
                "value": 50
              },
              {
                "color": "YELLOW",
                "direction": "BELOW",
                "label": "",
                "value": 0
              }
            ],
            "timeSeriesQuery": {
              "prometheusQuery": "100 * mysql_global_status_threads_connected{${Cluster},${Location},${Namespace}} / mysql_global_variables_max_connections{${Cluster},${Location},${Namespace}}\n"
            }
          },
          "title": "Available Connections"
        },
        "width": 5,
        "xPos": 0,
        "yPos": 3
      },
      {
        "height": 3,
        "widget": {
          "scorecard": {
            "blankView": {},
            "timeSeriesQuery": {
              "prometheusQuery": "mysql_global_variables_open_files_limit{${Cluster},${Location},${Namespace}} - mysql_global_variables_innodb_open_files{${Cluster},${Location},${Namespace}}\n"
            }
          },
          "title": "Open Files Available"
        },
        "width": 5,
        "xPos": 0,
        "yPos": 6
      },
      {
        "height": 3,
        "widget": {
          "title": "Slow Queries",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate(mysql_global_status_slow_queries{${Cluster},${Location},${Namespace}}[5m])"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 7,
        "xPos": 5,
        "yPos": 6
      },
      {
        "height": 3,
        "widget": {
          "title": "Cache Hit Rate",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate(mysql_global_status_table_open_cache_hits{${Cluster},${Location},${Namespace}}[5m]) /\n(rate(mysql_global_status_table_open_cache_hits{${Cluster},${Location},${Namespace}}[5m]) + rate(mysql_global_status_table_open_cache_misses{${Cluster},${Location},${Namespace}}[5m]))"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 6,
        "xPos": 0,
        "yPos": 9
      },
      {
        "height": 3,
        "widget": {
          "title": "Buffer Pool Cache Hit Rate",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "(rate(mysql_global_status_innodb_buffer_pool_read_requests{${Cluster},${Location},${Namespace}}[5m]) -\nrate(mysql_global_status_innodb_buffer_pool_reads{${Cluster},${Location},${Namespace}}[5m])) / rate(mysql_global_status_innodb_buffer_pool_read_requests{${Cluster},${Location},${Namespace}}[5m])"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 6,
        "xPos": 6,
        "yPos": 9
      },
      {
        "height": 3,
        "widget": {
          "title": "Full Join",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate(mysql_global_status_select_full_join{${Cluster},${Location},${Namespace}}[5m])"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 3,
        "xPos": 0,
        "yPos": 12
      },
      {
        "height": 3,
        "widget": {
          "title": "Full Range Join",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate(mysql_global_status_select_full_range_join{${Cluster},${Location},${Namespace}}[5m])"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 3,
        "xPos": 3,
        "yPos": 12
      },
      {
        "height": 3,
        "widget": {
          "title": "Range Check",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate(mysql_global_status_select_range_check{${Cluster},${Location},${Namespace}}[5m])"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 3,
        "xPos": 6,
        "yPos": 12
      },
      {
        "height": 3,
        "widget": {
          "title": "Scan",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate(mysql_global_status_select_scan{${Cluster},${Location},${Namespace}}[5m])"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 3,
        "xPos": 9,
        "yPos": 12
      }
    ]
  }
}
