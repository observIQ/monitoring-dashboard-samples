        {
          "height": 4,
          "widget": {
            "title": "Query Count",
            "xyChart": {
              "chartOptions": {
                "mode": "COLOR"
              },
              "dataSets": [
                {
                  "legendTemplate": "${metric.labels.apachedruid_service}",
                  "minAlignmentPeriod": "60s",
                  "plotType": "LINE",
                  "targetAxis": "Y1",
                  "timeSeriesQuery": {
                    "timeSeriesFilter": {
                      "aggregation": {
                        "alignmentPeriod": "60s",
                        "crossSeriesReducer": "REDUCE_SUM",
                        "groupByFields": [
                          "metric.label.\"apachedruid_service\""
                        ],
                        "perSeriesAligner": "ALIGN_NEXT_OLDER"
                      },
                      "filter": "metric.type=\"workload.googleapis.com/apachedruid.query_count\" resource.type=\"generic_node\"",
                      "secondaryAggregation": {
                        "alignmentPeriod": "60s",
                        "perSeriesAligner": "ALIGN_NONE"
                      }
                    }
                  }
                }
              ],
              "thresholds": [],
              "timeshiftDuration": "0s",
              "yAxis": {
                "label": "",
                "scale": "LINEAR"
              }
            }
