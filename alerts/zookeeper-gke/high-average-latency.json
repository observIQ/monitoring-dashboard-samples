{
  "displayName": "Prometheus - Zookeeper - High Average Latency",
  "documentation": {
    "content": "If `prometheus.googleapis.com/avg_latency/gauge` is 100ms or higher, it shows that the server can't keep up with demand and performance improvements should be sought after.",
    "mimeType": "text/markdown"
  },
  "userLabels": {},
  "conditions": [
    {
      "displayName": "Prometheus Target - prometheus/avg_latency/gauge",
      "conditionThreshold": {
        "aggregations": [
          {
            "alignmentPeriod": "300s",
            "perSeriesAligner": "ALIGN_MEAN"
          }
        ],
        "comparison": "COMPARISON_GT",
        "duration": "300s",
        "filter": "resource.type = \"prometheus_target\" AND metric.type = \"prometheus.googleapis.com/avg_latency/gauge\"",
        "thresholdValue": 100,
        "trigger": {
          "count": 1
        }
      }
    }
  ],
  "alertStrategy": {
    "autoClose": "604800s"
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": []
}
