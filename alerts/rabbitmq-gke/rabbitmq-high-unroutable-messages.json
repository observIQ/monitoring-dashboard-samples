{
  "displayName": "Prometheus - RabbitMQ - high unroutable messages",
  "documentation": {
    "content": "When the rate of unroutable messages is greater than a 0, that means consumers are not getting every message.",
    "mimeType": "text/markdown"
  },
  "userLabels": {},
  "conditions": [
    {
      "displayName": "Prometheus Target - prometheus/rabbitmq_channel_messages_unroutable_dropped_total/counter",
      "conditionThreshold": {
        "aggregations": [
          {
            "alignmentPeriod": "300s",
            "perSeriesAligner": "ALIGN_RATE"
          }
        ],
        "comparison": "COMPARISON_GT",
        "duration": "0s",
        "filter": "resource.type = \"prometheus_target\" AND metric.type = \"prometheus.googleapis.com/rabbitmq_channel_messages_unroutable_dropped_total/counter\"",
        "trigger": {
          "count": 1
        }
      }
    }
  ],
  "alertStrategy": {
    "autoClose": "604800s"
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": []
}
