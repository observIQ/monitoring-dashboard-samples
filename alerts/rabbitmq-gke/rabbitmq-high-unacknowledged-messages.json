{
  "displayName": "Prometheus - RabbitMQ - high unacknowledged messages",
  "documentation": {
    "content": "When the message unacknowledged rate is higher than a threshold, then errors or performance issues may arise.",
    "mimeType": "text/markdown"
  },
  "userLabels": {},
  "conditions": [
    {
      "displayName": "Prometheus Target - prometheus/rabbitmq_channel_messages_unacked/gauge",
      "conditionThreshold": {
        "aggregations": [
          {
            "alignmentPeriod": "300s",
            "perSeriesAligner": "ALIGN_MEAN"
          }
        ],
        "comparison": "COMPARISON_GT",
        "duration": "0s",
        "filter": "resource.type = \"prometheus_target\" AND metric.type = \"prometheus.googleapis.com/rabbitmq_channel_messages_unacked/gauge\"",
        "thresholdValue": 5,
        "trigger": {
          "count": 1
        }
      }
    }
  ],
  "alertStrategy": {
    "autoClose": "604800s"
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": []
}
